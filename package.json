{
  "name": "vue-setup",
  "version": "1.0.0",
  "description": "A basic set up of viewjs with webpack, babel, and mocha",
  "main": "main.js",
  "dependencies": {
    "vue": "2.6.5"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-loader": "8.0.5",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "css-loader": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "13.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "5.2.0",
    "mocha-loader": "2.0.0",
    "vue-loader": "15.6.2",
    "vue-template-compiler": "2.6.5",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "scripts": {
    "test": "webpack --mode development --watch --config test/webpack.config.js",
    "webpack": "webpack --mode development",
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "reinstall": "rm -rf node_modules && rm -rf package-lock.json && npm install"
  },
  "browserslist": "> 0.25%, not dead",
